{
  "comments": {
    "blockComment": [
      ["<%!--", "--%>"],
      ["<%#", "%>"]
    ]
  },
  "brackets": [
    ["<", ">"],
    ["{", "}"],
    ["(", ")"],
    ["[", "]"],
    ["do", "end"],
    ["fn", "end"]
  ],
  "colorizedBracketPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["do", "end"],
    ["fn", "end"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<%", " %>"],
    ["'", "'"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["'", "'"],
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["<", ">"],
    ["%", "%"],
    ["\"", "\""]
  ],
  "onEnterRules": [
    {
      "beforeText": {
        "pattern": "<(?!(?:area|base|br|col|embed|hr|img|input|keygen|link|menuitem|meta|param|source|track|wbr))([_:\\w][_:\\w-.\\d]*)(?:(?:[^'\"/>]|\"[^\"]*\"|'[^']*')*?(?!\\/)>)[^<]*$",
        "flags": "i"
      },
      "afterText": {
        "pattern": "^<\\/([_:\\w][_:\\w-.\\d]*)\\s*>",
        "flags": "i"
      },
      "action": {
        "indent": "indentOutdent"
      }
    },
    {
      "beforeText": "<%#\\s*$",
      "action": {
        "indent": "indentOutdent"
      },
      "afterText": "%>\\s*$"
    }
  ]
}
